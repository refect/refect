{
  // "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "valid-jsdoc": 2,
    "comma-dangle": 0,
    "prefer-template": 0,
    "arrow-body-style": 0,

    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/no-did-mount-set-state": 1,
    "react/prop-types": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,

    "strict": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 1,
    "no-empty-function": 1,
    // "import/no-unresolved": [2, {
    //   "ignore": [
    //     "^effects",
    //     "^utils",
    //     "refect"
    //   ]
    // }]
  },
  "plugins": [
    "react"
  ]
}
